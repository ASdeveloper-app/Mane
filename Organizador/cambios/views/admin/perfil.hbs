<h3>Mis Eventos</h3>
<div class="Eventos">
    <table class="table">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Editar</th>
                <th>Eliminar</th>
            </tr>
        </thead>
        <tbody>
            {{#each events}}
            <tr>
                <th> {{ this.name }} </th>
                <th> <a href="/admin/eventos/{{this._id}}/delete">Delete</a> </th>
                <th> <a href="/admin/eventos/detalle/{{this._id}}">Edit</a> </th>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <a href="eventos/datos" class="btn btn-primary">Nuevo Evento</a>

